<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7F7F7"
    tools:context=".HomeActivity">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="245dp"
        android:layout_height="59dp"
        android:text="@string/Bienvenida"
        android:textColor="#35595F"
        android:textSize="17sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="127dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="97dp"
        android:layout_height="69dp"
        android:contentDescription="@string/practice1"
        app:srcCompat="@drawable/Practice_foreground"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="580dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="90dp"
        android:layout_height="67dp"
        android:contentDescription="@string/todo1"
        app:srcCompat="@drawable/Profile_foreground"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,DuplicateSpeakableTextCheck"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="579dp" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="97dp"
        android:layout_height="69dp"
        android:contentDescription="@string/homeI"
        app:srcCompat="@drawable/Icone_foreground"
        tools:ignore="ImageContrastCheck"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="579dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="86dp"
        android:layout_height="39dp"
        android:text="@string/inerziA"
        android:textColor="#35595F"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="49dp" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="365dp"
        android:layout_height="98dp"
        android:background="#BBDEFB"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="202dp">

        <TextView
            android:id="@+id/textView"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:text="@string/unidad1"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:text="@string/saludos1"
            android:textSize="15sp"
            android:textStyle="bold" />

        <ProgressBar
            android:id="@+id/progressBar4"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_margin="6dp"
            android:max="100"
            android:progress="85"
            android:progressTint="#63C29C" />

        <EditText
            android:id="@+id/editTextNumberSigned"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:autofillHints=""
            android:ems="10"
            android:inputType="numberSigned" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="364dp"
        android:layout_height="92dp"
        android:background="#BBDEFB"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="317dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="153dp"
            android:layout_height="30dp"
            android:text="@string/unidad2"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="153dp"
            android:layout_height="37dp"
            android:text="@string/pronombresU2"
            android:textSize="15sp"
            android:textStyle="bold" />

        <ProgressBar
            android:id="@+id/progressBar5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:layout_margin="6dp"
            android:max="100"
            android:progress="60"
            android:progressTint="#63C29C" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="360dp"
        android:layout_height="96dp"
        android:background="#BBDEFB"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="427dp">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="153dp"
            android:layout_height="39dp"
            android:text="@string/unidad3"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="156dp"
            android:layout_height="33dp"
            android:text="@string/viajesU1"
            android:textSize="15sp"
            android:textStyle="bold" />

        <ProgressBar
            android:id="@+id/progressBar6"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:layout_margin="6dp"
            android:max="100"
            android:progress="15"
            android:progressTint="#63C29C" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="135dp"
        android:layout_height="117dp"
        android:contentDescription="@string/todo2"
        app:srcCompat="@mipmap/Logodefinitivo"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="27dp" />

</androidx.constraintlayout.widget.ConstraintLayout>